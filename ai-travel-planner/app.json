{
  "expo": {
    "name": "SuroyAI",
    "slug": "suroyai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.aitravelplanner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "package": "com.anonymous.aitravelplanner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-font",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c36b49ee-e099-43c8-87d9-d301f71b8e7c"
      }
    },
    "splash": {
      "image": "./assets/images/adaptive-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "owner": "aasiaboc"
  }
}
